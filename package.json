{
  "name": "react-crutch-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Zelofff/react-crutch-boilerplate.git",
  "author": "zeloff <zelofflab@yandex.ru>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "analyze": "webpack --config ./config/webpack.analyze.js",
    "test": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx}": [
      "prettier --write",
      "eslint ./src --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "react-testing-library": "^6.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
